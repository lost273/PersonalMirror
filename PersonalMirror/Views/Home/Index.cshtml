@{int[] array = { 99, -10, 100123, 18, -978, 5623, 463, -9, 287, 49 };
    }
<div class="container-fluid">
    <div class="row">
        @foreach (int i in array) {
            <div class="col-md-1">
                <div class="well">@i</div>
            </div>
        }
    </div>
</div>
<br/>
<div class="container-fluid">
    <div class="row">
        @for (int i = 0; i < array.Length; i++) {
            <div class="col-md-1">
                <div id="@i" class="well"></div>
         </div>
        }
    </div>
</div>

<div class="container">
    <div class="row">
        <div class="col-md-1" id="square-empty"></div>
        <div class="col-md-1" id="square" onclick="move()">1</div>
        <div class="col-md-1" id="square" onclick="move()">2</div>
    </div>
    <div class="row">
        <div class="col-md-1" id="square" onclick="move()">3</div>
        <div class="col-md-1" id="square" onclick="move()">4</div>
        <div class="col-md-1" id="square" onclick="move()">5</div>
    </div>
    <div class="row">
        <div class="col-md-1" id="square" onclick="move()">6</div>
        <div class="col-md-1" id="square" onclick="move()">7</div>
        <div class="col-md-1" id="square" onclick="move()">8</div>
    </div>
</div>




@section Scripts {
    @Scripts.Render("~/scripts/myscript.js")
    @Scripts.Render("~/scripts/movesquare.js")
}


@{int[] array = { 99, -10, 10012, 18, -978, 5623, 463, -9, 287, 49 };
    }
<h2>Current array:</h2>
<div class="container-fluid">
    <div class="row">
        @foreach (int i in array) {
            <div class="col-md-1">
                <div class="well">@i</div>
            </div>
        }
    </div>
</div>
<br/>

<div class="container-fluid">
    <div class="row">
        @for (int i = 0; i < array.Length; i++) {
            <div class="col-md-1">
                <div id="@i" class="well"></div>
         </div>
        }
    </div>
</div>
    


@section Scripts {
   @Scripts.Render("~/scripts/myscript.js")
}

